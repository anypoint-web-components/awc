{
  "name": "@anypoint-web-components/awc",
  "version": "2.0.5",
  "description": "Anypoint web components - base components library",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": {
    "name": "Pawel Uchida-Psztyc",
    "email": "pawel.psztyc@gmail.com"
  },
  "contributors": [
    "Your name can be here!"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/anypoint-web-components/awc.git"
  },
  "bugs": {
    "url": "https://github.com/anypoint-web-components/awc/issues",
    "email": "arc@mulesoft.com"
  },
  "scripts": {
    "analyze": "cem analyze --litelement",
    "build": "tsc",
    "prepublish": "tsc && tsc:add-runtime-types && npm run analyze -- --exclude dist",
    "start": "web-dev-server --app-index demo/index.html --node-resolve --open --watch --root-dir .",
    "start:ts": "concurrently --raw \"tsc --watch --preserveWatchOutput\" \"wds --app-index demo/index.html --node-resolve --open --watch --root-dir .\"",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "lint:types": "tsc",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "test": "tsc && wtr --coverage --playwright --browsers chromium firefox webkit",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch --playwright --browsers chromium\"",
    "prepare": "husky install",
    "tsc": "tsc --watch",
    "tsc:add-runtime-types": "node scripts/prePublish.js"
  },
  "dependencies": {
    "@open-wc/dedupe-mixin": "^1.3.0",
    "lit": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@open-wc/eslint-config": "^7.0.0",
    "@open-wc/testing": "3.1.2",
    "@pawel-up/data-mock": "^0.2.0",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@web/dev-server": "^0.1.30",
    "@web/dev-server-esbuild": "^0.3.0",
    "@web/test-runner": "0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prismjs": "^1.27.0",
    "sinon": "^13.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "customElements": "custom-elements.json",
  "keywords": [
    "web-components",
    "anypoint",
    "icons"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "exports": {
    ".": "./dist/index.js",
    "./define/anypoint-autocomplete.js": "./dist/define/anypoint-autocomplete.js",
    "./define/anypoint-button.js": "./dist/define/anypoint-button.js",
    "./define/anypoint-checkbox.js": "./dist/define/anypoint-checkbox.js",
    "./define/anypoint-chip-input.js": "./dist/define/anypoint-chip-input.js",
    "./define/anypoint-chip.js": "./dist/define/anypoint-chip.js",
    "./define/anypoint-combobox.js": "./dist/define/anypoint-combobox.js",
    "./define/anypoint-dialog-scrollable.js": "./dist/define/anypoint-dialog-scrollable.js",
    "./define/anypoint-dialog.js": "./dist/define/anypoint-dialog.js",
    "./define/anypoint-dropdown-menu.js": "./dist/define/anypoint-dropdown-menu.js",
    "./define/anypoint-dropdown.js": "./dist/define/anypoint-dropdown.js",
    "./define/anypoint-icon-button.js": "./dist/define/anypoint-icon-button.js",
    "./define/anypoint-icon-item.js": "./dist/define/anypoint-icon-item.js",
    "./define/anypoint-input-combobox.js": "./dist/define/anypoint-input-combobox.js",
    "./define/anypoint-input.js": "./dist/define/anypoint-input.js",
    "./define/anypoint-item-body.js": "./dist/define/anypoint-item-body.js",
    "./define/anypoint-item.js": "./dist/define/anypoint-item.js",
    "./define/anypoint-listbox.js": "./dist/define/anypoint-listbox.js",
    "./define/anypoint-masked-input.js": "./dist/define/anypoint-masked-input.js",
    "./define/anypoint-menu-button.js": "./dist/define/anypoint-menu-button.js",
    "./define/anypoint-progress.js": "./dist/define/anypoint-progress.js",
    "./define/anypoint-radio-button.js": "./dist/define/anypoint-radio-button.js",
    "./define/anypoint-radio-group.js": "./dist/define/anypoint-radio-group.js",
    "./define/anypoint-selector.js": "./dist/define/anypoint-selector.js",
    "./define/anypoint-switch.js": "./dist/define/anypoint-switch.js",
    "./define/anypoint-tab.js": "./dist/define/anypoint-tab.js",
    "./define/anypoint-tabs.js": "./dist/define/anypoint-tabs.js",
    "./define/anypoint-textarea.js": "./dist/define/anypoint-textarea.js",
    "./define/bottom-sheet.js": "./dist/define/bottom-sheet.js",
    "./define/color-input-selector.js": "./dist/define/color-input-selector.js",
    "./define/color-selector.js": "./dist/define/color-selector.js",
    "./define/date-time.js": "./dist/define/date-time.js",
    "./define/focusable-helper.js": "./dist/define/focusable-helper.js",
    "./define/material-ripple.js": "./dist/define/material-ripple.js",
    "./define/overlay-backdrop.js": "./dist/define/overlay-backdrop.js",
    "./define/overlay-manager.js": "./dist/define/overlay-manager.js",
    "./define/scroll-threshold.js": "./dist/define/scroll-threshold.js",
    "./define/star-rating.js": "./dist/define/star-rating.js"
  }
}
